<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>lgServlet</servlet-name>
    <servlet-class>com.lagou.edu.framework.servlet.LgDispatcherServlet</servlet-class>
    <init-param>
      <param-name>propertiesConfigLocation</param-name>
      <param-value>springmvc.properties</param-value>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>lgServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>


</web-app>
